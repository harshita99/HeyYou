<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dopamineproject">

    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mainicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Quote"/>
        <activity android:name=".movie">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="https" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Next" />
        <activity android:name=".MentalTest" />
        <activity android:name=".MainDraw" />
        <activity android:name=".choose" />
        <activity android:name=".writeSomething" />
        <activity android:name=".readJournal" />
        <activity android:name=".DaysList" />
        <activity android:name=".TrackMood"/>
        <receiver android:name=".Receiver2" />
        <receiver android:name=".receiver1" />
        <receiver android:name=".Receiver3" />
        <activity android:name=".Helpline"/>
    </application>

</manifest>